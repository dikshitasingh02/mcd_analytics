-- join to get review counts per store
SELECT s.city, s.subdivision, COUNT(r.reviewer_id) AS review_count, AVG(r.rating_count) AS avg_rating
FROM stores s
LEFT JOIN reviews r ON s.address = r.store_address
GROUP BY s.city, s.subdivision
ORDER BY avg_rating DESC;
</sql><current_tab id="1"/></tab_sql></sqlb_project>
